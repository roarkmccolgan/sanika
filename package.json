        "lodash": "^4.17.4",
        "postcss-cssnext": "^3.1",
        "postcss-import": "^11.0",
        "tailwindcss": ">=0.1.0",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.3",
        "@fortawesome/fontawesome-pro-regular": "^5.0.6",
        "@fortawesome/vue-fontawesome": "0.0.22",
        "npm": "^5.10.0",
        "vue-carousel": "^0.8.1",
        "vue-instantsearch": "^1.5.1",
        "vue-sweetalert2": "^1.2.4",
